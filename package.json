{
  "name": "okr-tracker",
  "version": "1.4.2",
  "private": true,
  "scripts": {
    "dev": "firebase emulators:start --import=./tests & vue-cli-service serve",
    "dev:test": "firebase emulators:start --import=./tests & vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:e2e": "firebase emulators:start --import=./tests & vue-cli-service test:e2e --mode e2e",
    "test:e2e:headless": "firebase emulators:exec --project origo-okr-tracker 'node ./src/mock & (sleep 15 && vue-cli-service test:e2e --mode e2e --headless)'",
    "deploy": "npm run build && firebase deploy --project=production",
    "lint": "vue-cli-service lint",
    "lint:style": "stylelint 'src/**/*.scss' 'src/**/*.vue'",
    "lint:style:fix": "stylelint 'src/**/*.scss' 'src/**/*.vue' --fix"
  },
  "dependencies": {
    "@braid/griddle": "^1.3.0",
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@fortawesome/free-regular-svg-icons": "5.15.1",
    "babel-runtime": "^6.26.0",
    "core-js": "^3.6.5",
    "d3": "^6.2.0",
    "date-fns": "^2.16.1",
    "dompurify": "^2.2.0",
    "firebase": "^8.0.0",
    "flatpickr": "^4.6.6",
    "fuse.js": "^6.4.2",
    "image-file-resize": "^1.0.3",
    "marked": "^1.2.2",
    "v-tooltip": "^2.0.3",
    "vee-validate": "^3.4.2",
    "vue": "^2.6.12",
    "vue-click-outside": "^1.1.0",
    "vue-flatpickr-component": "^8.1.6",
    "vue-i18n": "^8.22.1",
    "vue-meta": "^2.4.0",
    "vue-resize": "^0.5.0",
    "vue-router": "^3.4.7",
    "vue-select": "^3.10.8",
    "vue-toasted": "^1.1.28",
    "vuefire": "^2.2.4",
    "vuex": "^3.5.1",
    "vuexfire": "^3.2.4"
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "^1.0.8",
    "@vue/cli-plugin-babel": "^4.5.8",
    "@vue/cli-plugin-e2e-cypress": "^4.5.8",
    "@vue/cli-plugin-eslint": "^4.5.8",
    "@vue/cli-service": "^4.5.8",
    "@vue/eslint-config-airbnb": "^5.1.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "data-dir": "^1.2.3",
    "eslint": "^7.11.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.1.0",
    "firebase-tools": "^8.14.1",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.2",
    "sass-loader": "^10.0.3",
    "stylelint": "^13.7.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "vue-template-compiler": "^2.6.12"
  }
}
